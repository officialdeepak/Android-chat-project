<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yupchat">

    <application
        android:allowBackup="true"
        android:icon="@drawable/chat"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ForgetPassword"></activity>
        <activity android:name=".AboutActivity" />
        <activity android:name=".Privacypolicy" />
        <activity android:name=".helpActivity" />
        <activity android:name=".SettingActivities" />
        <activity android:name=".GroupChat" />
        <activity android:name=".ProfileDetailactivity" />
        <activity android:name=".SignInActivities">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivities" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>